{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "githubActionArtifacts",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:DeleteObject"
      ],
      "Resource": [
        "${s3_arn}/*",
        "${s3_arn}",
        "${s3_packages_arn}/*",
        "${s3_packages_arn}"
      ]
    }
  ]
}